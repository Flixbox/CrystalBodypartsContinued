  <HediffDef ParentName="AddedAnimalCrystalBodyPartBase">
    <defName>AnimalCrystalArm</defName>
    <label>animal crystal arm</label>
    <description>An installed animal crystal bodypart.</description>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>animal crystal arm</label>
            <capacities>
              <li>Scratch</li>
            </capacities>
            <power>15</power>
            <cooldownTime>1.4</cooldownTime>
            <alwaysTreatAsWeapon>false</alwaysTreatAsWeapon>
          </li>
        </tools>
      </li>
    </comps>
    <spawnThingOnRemoved>AnimalCrystalArm</spawnThingOnRemoved>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.25</partEfficiency>
    </addedPartProps>
    <stages>
      <li>
        <statOffsets>
          <ArmorRating_Sharp>0.05</ArmorRating_Sharp>
          <ArmorRating_Blunt>0.05</ArmorRating_Blunt>
          <ArmorRating_Heat>0.05</ArmorRating_Heat>
        </statOffsets>
      </li>
    </stages>
  </HediffDef>

